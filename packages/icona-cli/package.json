{
  "name": "@icona/cli",
  "version": "0.0.10",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/icona.git",
    "directory": "packages/icona-cli"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/src/index.js",
      "import": "./dist/src/index.mjs"
    }
  },
  "main": "./dist/src/index.mjs",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "icona": "./bin/index.mjs"
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "build": "yarn build:icona-cli && yarn build:tsc",
    "build:icona-cli": "node build.mjs",
    "build:package": "yarn build",
    "build:tsc": "tsc --emitDeclarationOnly",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@icona/generator": "^0.0.10",
    "@icona/utils": "^0.0.10",
    "commander": "^10.0.1",
    "dedent": "^0.7.0",
    "findup-sync": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@icona/types": "^0.0.11",
    "@types/dedent": "^0.7.0",
    "@types/findup-sync": "^4.0.2",
    "@types/node": "^20.2.5",
    "esbuild": "^0.17.19",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.5.1",
  "publishConfig": {
    "access": "public"
  }
}
